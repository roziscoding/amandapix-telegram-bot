{
  "name": "amandapix-telegram-bot",
  "version": "1.2.0",
  "description": "Telegram bot that generates Pix codes",
  "scripts": {
    "test": "mocha",
    "lint": "eslint .",
    "clean": "rm -rf dist",
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "prepack": "npm run lint && npm test && npm run build:clean",
    "dev": "tsnd src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roziscoding/amandapix-telegram-bot.git"
  },
  "keywords": [],
  "author": "Roz <hello@roz.ninja>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/roziscoding/amandapix-telegram-bot/issues"
  },
  "homepage": "https://github.com/roziscoding/amandapix-telegram-bot#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.1",
    "@types/mongodb": "^3.6.20",
    "@types/node": "^15.0.1",
    "@types/qrcode": "^1.4.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vercel/node": "^1.10.0",
    "chai": "^4.3.4",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^6.0.0",
    "mocha": "^8.3.2",
    "semantic-release": "^17.4.7",
    "ts-node-dev": "^2.0.0",
    "typegram": "^3.3.1",
    "typescript": "^4.7.4",
    "vercel": "^25.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@grammyjs/router": "^2.0.0",
    "axios": "^0.21.1",
    "dotenv": "^16.0.1",
    "grammy": "^1.10.1",
    "jimp": "^0.16.1",
    "mathjs": "^9.4.3",
    "mongodb": "^3.6.7",
    "pix-me": "^2.0.0",
    "qrcode": "^1.4.4",
    "sugar-env": "^1.5.14",
    "uuid": "^8.3.2"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}