{
  "name": "amandapix-telegram-bot",
  "version": "1.2.0",
  "description": "Telegram bot that generates Pix codes",
  "scripts": {
    "test": "mocha",
    "lint": "eslint .",
    "clean": "rm -rf dist",
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "prepack": "npm run lint && npm test && npm run build:clean",
    "dev": "tsnd src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roziscoding/amandapix-telegram-bot.git"
  },
  "keywords": [],
  "author": "Roz <hello@roz.ninja>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/roziscoding/amandapix-telegram-bot/issues"
  },
  "homepage": "https://github.com/roziscoding/amandapix-telegram-bot#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/common-tags": "^1.8.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.7.14",
    "@types/qrcode": "^1.5.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vercel/node": "^2.5.13",
    "chai": "^4.3.6",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "semantic-release": "^19.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2",
    "vercel": "^28.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@grammyjs/conversations": "^1.0.2",
    "@grammyjs/router": "^2.0.0",
    "@grammyjs/storage-mongodb": "^2.0.0",
    "axios": "^0.27.2",
    "common-tags": "^1.8.2",
    "dotenv": "^16.0.2",
    "grammy": "^1.11.0",
    "jimp": "^0.16.1",
    "mathjs": "^11.1.0",
    "mongodb": "^4.9.1",
    "pix-me": "^2.0.0",
    "qrcode": "^1.5.1",
    "sugar-env": "^1.5.14",
    "uuid": "^8.3.2"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}